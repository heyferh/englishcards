<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.feku.englishcards">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/MyTheme">
        <activity android:name=".activity.MainActivity"></activity>
        <activity android:name=".activity.SelectDictionaryActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.RegularCardActivity"></activity>
        <activity android:name=".activity.FavouriteCardActivity"></activity>
        <activity android:name=".activity.LeitnerModeActivity"></activity>
    </application>

</manifest>
